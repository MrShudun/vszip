{
	"name": "vszip",
	"displayName": "vszip",
	"description": "zip 工具箱",
	"publisher": "shudun",
	"version": "1.1.0",
	"engines": {
		"vscode": "^1.10.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vszip.compress"
	],
	"main": "./extension.js",
	"icon": "ico.png",
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/youngDun/vszip.git"
	},
	"keywords": [
		"vs code",
		"visual studio code",
		"zip"
	],
	"author": "shudun",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/youngDun/vszip/issues"
	},
	"homepage": "https://github.com/youngDun/vszip#readme",
	"contributes": {
		"commands": [
			{
				"command": "vszip.compress",
				"title": "zip压缩"
			}
		],
		"menus": {
			"explorer/context": [
